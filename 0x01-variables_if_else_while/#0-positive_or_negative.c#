#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */

int main(void)
{
  int n, dig;
  srand(time(0));
  n = rand() - RAND_MAX / 2;
  dig = n % 10;
  if (n > 0)
    printf("%i is positive\n", (n%10));
    else if (n == 0)
        printf("%i is zero\n", n);
    else
        printf("%i is negative\n", n);
    return (0);
}
